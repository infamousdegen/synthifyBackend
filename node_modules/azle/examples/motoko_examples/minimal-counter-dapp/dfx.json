{
    "canisters": {
        "azle": {
            "type": "custom",
            "build": "npx azle azle",
            "root": "canisters/azle",
            "ts": "canisters/azle/index.ts",
            "candid": "canisters/azle/index.did",
            "wasm": ".azle/azle/azle.wasm.gz",
            "declarations": {
                "output": "dfx_generated/azle",
                "node_compatibility": true
            }
        },
        "minimal_dapp_assets": {
            "frontend": {
                "entrypoint": "canisters/minimal_dapp_assets/src/index.html"
            },
            "source": [
                "canisters/minimal_dapp_assets/assets",
                "dist/minimal_dapp_assets/"
            ],
            "type": "assets"
        },
        "motoko": {
            "main": "canisters/motoko/Main.mo",
            "type": "motoko",
            "declarations": {
                "output": "dfx_generated/motoko",
                "node_compatibility": true
            }
        },
        "rust": {
            "candid": "canisters/rust/src/lib.did",
            "package": "rust",
            "type": "rust",
            "declarations": {
                "output": "dfx_generated/rust",
                "node_compatibility": true
            }
        }
    }
}
